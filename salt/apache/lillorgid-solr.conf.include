
    <Location "/">

        ProxyPass http://localhost:8983/

        ProxyPreserveHost on

        AuthName "LILL OrgId Solr"
        AuthType Basic
        AuthUserFile /home/{{ user }}/htpasswd
        require valid-user

    </Location>

    <Location "/.well-known/acme-challenge">
            ProxyPass !
            Require all granted
    </Location>

